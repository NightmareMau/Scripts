-- Scripts:

local function EQCL_fake_script() -- Main.LocalScript 
	local script = Instance.new('LocalScript', Main)

	local Margin = script.Parent
	local Input = Margin.Input
	local Display = Margin.Display
	
	local Colors = {
		[Color3.fromRGB(248, 109, 124)] = {
			"local",
			"while",
			"true",
			"function",
			"return",
			"game",
			"error",
			"for", 
			"in",
			"do", 
			"end"
		},
		[Color3.fromRGB(85, 170, 255)] = {
				"Color3",
				"pairs",
				"ipairs",
				"fromRGB",
			"require",
			"print",
			"warn",
			"script"
		},
	}
	
	local ColorizePattern = '<font color="rgb(%d, %d, %d)">%s</font>'
	
	local function coloriar(keyword, color)
		return string.format(ColorizePattern, color.r*255, color.g*255, color.b*255, keyword)
	end
	
	local function textoinsano(texto)
		for color, keywords in pairs(Colors) do
			for _, keyword in pairs(keywords) do
				texto = string.gsub(texto, keyword, coloriar(keyword, color))
			end
		end
		return texto
	end
	
	local function InputChanged() 
			Display.Text = textoinsano(Input.Text)
	end
	
	Input:GetPropertyChangedSignal("Text"):Connect(InputChanged)
end
coroutine.wrap(EQCL_fake_script)()
local function RECXM_fake_script() -- Main.ButtonHandler 
	local script = Instance.new('LocalScript', Main)

	script.Parent.Clear.MouseButton1Click:Connect(function()
		script.Parent.Display.Text = ""
		script.Parent.Input.Text = ""
	end)
	script.Parent.Execute.MouseButton1Click:Connect(function()
		script.Parent.RemoteEvent:FireServer(script.Parent.Input.Text)
	end)
	
	local x = true
	script.Parent.Parent["-"].MouseButton1Click:Connect(function()
		if x == true then
			if script.Parent.Parent.config.Size == UDim2.new(0.585, 0,6.721, 0) then
				game.TweenService:Create(script.Parent.Parent.config,TweenInfo.new(.1, Enum.EasingStyle.Sine),{Size = UDim2.new(.001,0,6.721,0)}):Play()
				task.wait(.1)
			end
			script.Parent.Parent.config.Visible = false
			game.TweenService:Create(script.Parent,TweenInfo.new(.1, Enum.EasingStyle.Sine),{Size = UDim2.new(1,0,1,0)}):Play()
			for _,v in pairs(script.Parent:GetChildren()) do
				if v:IsA("TextButton") or v:IsA("TextBox") then
					v.Visible = false
				end
			end
			x = false
		else
			game.TweenService:Create(script.Parent,TweenInfo.new(.1, Enum.EasingStyle.Sine),{Size = UDim2.new(1, 0,6.7, 0)}):Play()
			for _,v in pairs(script.Parent:GetChildren()) do
				if v:IsA("TextButton") or v:IsA("TextBox") then
					v.Visible = true
				end
			end
			script.Parent.Parent.config.Visible = true
			x = true
		end
	end)
	local y = false
	script.Parent.themes.MouseButton1Click:Connect(function()
		if script.Parent.Parent.config.Size == UDim2.new(0.585, 0,6.721, 0) then
			game.TweenService:Create(script.Parent.Parent.config,TweenInfo.new(.1, Enum.EasingStyle.Sine),{Size = UDim2.new(.001,0,6.721,0)}):Play()
			y = false
		else
			game.TweenService:Create(script.Parent.Parent.config,TweenInfo.new(.1, Enum.EasingStyle.Sine),{Size = UDim2.new(0.585, 0,6.721, 0)}):Play()
			y = true
		end
	end)
end
coroutine.wrap(RECXM_fake_script)()
local function VCIYS_fake_script() -- Topbar.Drag 
	local script = Instance.new('LocalScript', Topbar)

	local UserInputService = game:GetService("UserInputService")
	
	local gui = script.Parent
	
	local dragging
	local dragInput
	local dragStart
	local startPos
	
	local function update(input)
		local delta = input.Position - dragStart
		gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end
	
	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			update(input)
		end
	end)
end
coroutine.wrap(VCIYS_fake_script)()
local function RTVH_fake_script() -- config.Theme provider 
	local script = Instance.new('LocalScript', config)

	local x = script.Parent.Parent.Logo
	local y = script.Parent.Parent.Main.fondo
	local yy = script.Parent.Parent
	local z = {yy["-"], yy["Nombre"], yy.Main.Execute, yy.Main.Clear, yy.Main.themes}
	script.Parent.Kuromi.MouseButton1Click:Connect(function()
		x.Image = "rbxthumb://type=Asset&id=12155609393&w=150&h=150"
		y.Image = "rbxthumb://type=Asset&id=11872476797&w=150&h=150" 
		script.Parent.ImageLabel.Image = "rbxthumb://type=Asset&id=13027119561&w=150&h=150"  
		for _,v in pairs(z) do
			v.TextColor3 = Color3.fromRGB(85, 0, 255)
		end
	end)
	
	script.Parent["Toy chica"].MouseButton1Click:Connect(function()
		x.Image = "rbxthumb://type=Asset&id=3552511549&w=150&h=150"
		y.Image = "rbxthumb://type=Asset&id=5257430696&w=150&h=150" 
		script.Parent.ImageLabel.Image = "rbxthumb://type=Asset&id=1303476758&w=150&h=150"  
		for _,v in pairs(z) do
			v.TextColor3 = Color3.fromRGB(255, 255, 0)
		end
	end)
end
coroutine.wrap(RTVH_fake_script)()
